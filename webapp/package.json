{
  "name": "dietintel-webapp",
  "version": "1.0.0",
  "description": "Express.js web application for DietIntel meal plan management",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm test && npm run test:e2e",
    "screenshots": "node ../scripts/capture-screenshots.js",
    "lint": "eslint .",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "test:profiles": "jest -- profiles"
  },
  "keywords": [
    "nutrition",
    "meal-planning",
    "dietintel",
    "express",
    "webapp"
  ],
  "author": "DietIntel Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "i18next": "^25.5.1",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.0",
    "sqlite3": "^5.1.7",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "playwright": "^1.55.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krukmat/diet-intel.git",
    "directory": "webapp"
  }
}
